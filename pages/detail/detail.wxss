/* pages/detail/detail.wxss */
.normal_style {
    width:100%;
    height:auto;
    background:#ffffff;
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
.border_bttom {
    /* border-bottom: 1px solid rgba(0,0,0,0.12); */
    box-shadow: inset 0 1px 1px -1px rgba(0,0,0,0.12);
    box-sizing: border-box
}
.swiper_content {
    height: 320px;
}
.swiper_image {
    display: block;
    width: 100%;
    height: 100%;
}
.head_info {
    padding: 0 16px;
    background-color: white;
}
.con_head{
    padding: 24px 0 16px;
    min-height: 105px;
}
.con_head h1 {
    font-size: 24px;
    line-height: 29px;
    font-weight: bold;
}
.con_head h3{
    font-size: 14px;
    line-height: 16px;
    color: rgba(0,0,0,0.54);
}

.price_box {
    display: flex;
    position: relative;
    margin-top: 16px
}
.price_box .currency {
    font-size: 24px;
    line-height: 29px;
    color: #ff5722;
    font-weight: bold;
    margin-right: 5px;  
}
.price_box .price_now{
    font-size: 24px;
    line-height: 29px;
    color: #ff5722;
    font-weight: bold;
    margin-right: 5px;
}
.price_box .price_before{
    margin-bottom: 3px;
    font-size: 14px;
    line-height: 16px;
    text-decoration: line-through;
    color: rgba(0,0,0,0.38);
    -ms-flex-item-align: end;
    align-self: flex-end;
}
.price_guarantee {
    margin-top: 8px;
    height: 16px;
    font-size: 12px;
    color: rgba(0,0,0,0.54)
}

.keynote{
    padding: 16px 0;
}
.klook-icon-people:before {
    content: "\E084";
}
.klook-icon-people:before {
    color: #d1d1d1;
}
.klook-icon-time:before {
    content: "\E084";
}
.klook-icon-time:before {
    color: #d1d1d1;
}
.keynote .note_des p{
    padding: 8px 0 8px 24px;
    font-size: 14px;
    line-height: 16px;
    color: rgba(0,0,0,0.54);
}

.act_icons {
    padding: 16px 0;
}
.act_icons>div{
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    padding: 10px 0;
}
.flex_vc, .flex_v_justify {
    -webkit-box-align: center;
    -moz-align-items: center;
    -ms-align-items: center;
    -o-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}
