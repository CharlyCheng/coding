.act_icons__icon {
    -webkit-box-flex: 0;
    -ms-flex: none;
    flex: none;
    font-size: 24px;
    color: #ff5722;
}
.klook-symbol {
    box-sizing: content-box;
    width: 1em;
    height: 1em;
    fill: currentColor;
    overflow: hidden;
}
.act_icons>div p {
    margin-left: 16px;
    font-size: 16px;
    line-height: 19px;
    color: rgba(0,0,0,0.87);
}
.high_light {
    padding: 24px 16px;
    background-color: #fff;
    font-size: 14px;
    line-height: 21px;
    position: relative
}
.high_light ul {
    padding-left: 24px;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    /* align-items: center; */
    /* margin-left: 12px; */
}
[data-dpr="2"] .mark_down ul, [data-dpr="2"] .mark_down ol {
    line-height: 21px;
}
.mark_down ul, [data-dpr="2"] .mark_down ol {
    font-size: 14px;
}
ul, p {
    margin: 0;
    padding: 0;
}
li {
    display: list-item;
    text-align: -webkit-match-parent;
    color: #333;
}
.mark_down ul li, .mark_down ol li {
    list-style: initial;
    margin-bottom: 7px;
}


.pac_info {
    margin-top: 8px;
    padding-top: 16px;
    background-color: #fff;
}
.block_head {
    padding: 13px 16px 14px 32px;
    background-color: #fff;
    font-size: 24px;
    line-height: 29px;
    font-weight: bold;
    position: relative;
}
.block_head::before {
    content: '';
    position: absolute;
    display: block;
    width: 4px;
    height: 20px;
    border-radius: 100px;
    background-color: #ff5722;
    left: 16px;
    top: 18px;
}
.pac_info_top {
    margin-top: 22px;
    padding: 16px;
}
.pac_info_spec__name {
    line-height: 19px;
    font-size: 16px;
    font-weight: 600;
    color: rgba(0,0,0,0.87);
}
.pac_info__content {
    padding: 16px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-content: center;
    background-color: #fafafa;
    border: 1px solid rgba(0,0,0,0.12);
}
.pac_info_spec__attrs {
    margin-top: 8px;
    font-size: 14px;
    line-height: 28px;
    color: rgba(0,0,0,0.54);
}